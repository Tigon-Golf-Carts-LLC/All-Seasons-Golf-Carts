<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/android-chrome-192x192.png"/>
      <square150x150logo src="/android-chrome-192x192.png"/>
      <square310x310logo src="/android-chrome-512x512.png"/>
      <wide310x150logo src="/android-chrome-512x512.png"/>
      <TileColor>#f97316</TileColor>
    </tile>
    <notification>
      <polling-uri src="https://allseasonsgolfcarts.com/notifications/tile1.xml"/>
      <polling-uri2 src="https://allseasonsgolfcarts.com/notifications/tile2.xml"/>
      <polling-uri3 src="https://allseasonsgolfcarts.com/notifications/tile3.xml"/>
      <polling-uri4 src="https://allseasonsgolfcarts.com/notifications/tile4.xml"/>
      <polling-uri5 src="https://allseasonsgolfcarts.com/notifications/tile5.xml"/>
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
    <badge>
      <polling-uri src="https://allseasonsgolfcarts.com/notifications/badge.xml"/>
      <frequency>30</frequency>
    </badge>
  </msapplication>
</browserconfig>
